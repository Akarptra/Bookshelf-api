{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "eol-last": 0,
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }],
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style" : 0,
        "no-console" : "off",
        "indent" : "off"
    }
}
